@model Common.ResultsModel.Comparison
@{
    ViewBag.Title = "History";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Common passages between @Model.SuspiciousDocumentName and @Model.OriginalDocumentName</h2>
<a asp-action="ComparedDocuments"
   asp-route-suspiciousName="@Model.SuspiciousDocumentName">
    Back to compared documents for @Model.SuspiciousDocumentName
</a>|<a asp-action="Index">
    Back to History
</a>
<table class="table">
    <tr>
        <th>@Model.OriginalDocumentName</th>
        <th>@Model.SuspiciousDocumentName</th>
        <th>Similarity Score</th>
    </tr>
    @foreach (var passage in Model.CommonPassages)
    {
        <tr>
            <td width="48%">
                @passage.OriginalDocumentPassage
            </td>
            <td width="48%">
                @passage.SuspiciousDocumentPassage
            </td>
            <td width="4%">
                @passage.SimilarityScore
            </td>
        </tr>

    }
</table>